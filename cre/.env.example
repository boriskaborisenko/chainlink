RPC_URL=https://sepolia.infura.io/v3/YOUR_KEY
CRE_SIGNER_PK=0x...
KYC_BROKER_ADDRESS=0x...
PASS_REGISTRY_ADDRESS=0x...

# Sumsub
SUMSUB_BASE_URL=https://api.sumsub.com
SUMSUB_APP_TOKEN=...
SUMSUB_SECRET_KEY=...
SUMSUB_SDK_TOKEN_PATH=/resources/accessTokens/sdk
SUMSUB_STATUS_PATH_TEMPLATE=/resources/applicants/-;externalUserId={userId}/one
# wallet_request (default) creates a fresh Sumsub applicant per KYC request in sandbox demos.
# wallet keeps a single long-lived applicant per wallet address.
SUMSUB_USER_ID_MODE=wallet_request

KYC_LEVEL_NAME=basic-kyc
TOKEN_TTL_SECONDS=600
POLL_INTERVAL_MS=5000
SYNC_POLL_INTERVAL_MS=120000
ATTESTATION_EXPIRATION_DAYS=180
FLAG_HUMAN=1
FLAG_WORLD_ID=2
STATE_FILE=.cre-state.json

# World ID (optional)
WORLD_ID_APP_ID=app_staging_xxx
WORLD_ID_ACTION=incognito-action
# Optional custom endpoint. Usually leave empty to use World default:
# WORLD_ID_VERIFY_ENDPOINT=https://developer.worldcoin.org/api/v2/verify/app_staging_xxx
WORLD_ID_VERIFY_ENDPOINT=
